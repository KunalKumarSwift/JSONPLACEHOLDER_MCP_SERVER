{
  "name": "jsonplaceholder-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for interacting with JSONPlaceholder API",
  "main": "build/index.js",
  "scripts": {
    "build": "esbuild index.js --bundle --outfile=build/index.js --platform=node --external:@modelcontextprotocol/sdk",
    "start": "node build/index.js",
    "test": "node tests/tests.js",
    "test:users": "node tests/users.test.js",
    "test:posts": "node tests/posts.test.js",
    "test:comments": "node tests/comments.test.js",
    "test:albums": "node tests/albums.test.js",
    "test:photos": "node tests/photos.test.js",
    "test:todos": "node tests/todos.test.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "esbuild": "^0.20.0"
  }
}
